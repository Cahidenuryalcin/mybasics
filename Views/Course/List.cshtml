@model List<Course>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>course list page</h1>
    
    @foreach(var kurs  in Model){
        <div>
            <p>@kurs.Id</p>
            <p>@kurs.Title</p>
            <p>@kurs.Description</p>
        </div>
    }

</body>
</html>